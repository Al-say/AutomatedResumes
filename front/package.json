{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node start-dev.mjs",
    "build": "next build",
    "build:prod": "next build && node scripts/copy-dist.mjs",
    "build:analyze": "ANALYZE=true next build",
    "start": "node start-prod.mjs",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out node_modules/.cache",
    "preview": "next start",
    "export": "next build && next export",
    "test": "echo \"No tests specified\" && exit 0",
    "test:watch": "echo \"No tests specified\" && exit 0",
    "size": "npx @next/bundle-analyzer",
    "deps:check": "npx depcheck",
    "deps:update": "npx npm-check-updates -u"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "chart.js": "^4.4.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.468.0",
    "next": "16.1.3",
    "next-themes": "^0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-icons": "^5.5.0",
    "sql.js": "^1.13.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.3",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "baseline-browser-mapping": "^2.9.15",
    "depcheck": "^1.4.7",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.0.1",
    "npm-check-updates": "^19.3.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.19",
    "typescript": "^5"
  }
}
